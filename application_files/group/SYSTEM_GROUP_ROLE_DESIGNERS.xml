<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<groupHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <group>
        <name>Designers</name>
        <securityMap>SECURITYMAP_TEAM</securityMap>
        <uuid>SYSTEM_GROUP_ROLE_DESIGNERS</uuid>
        <groupTypeUuid>SYSTEM_GROUP_TYPE_CUSTOM</groupTypeUuid>
        <description>Members of this group will be able to design applications.</description>
        <delegatedCreation>false</delegatedCreation>
        <memberPolicy>MEMBERPOLICY_CLOSED</memberPolicy>
        <viewingPolicy>VIEWINGPOLICY_LOW</viewingPolicy>
        <attributes/>
    </group>
    <members>
        <users/>
        <groups>
            <groupUuid>SYSTEM_GROUP_PROCESS_MODEL_CREATORS</groupUuid>
        </groups>
    </members>
    <admins>
        <users>
            <userUuid>Administrator</userUuid>
        </users>
        <groups/>
    </admins>
    <ruleSet>
        <expressions>
            <entityTypeName>u</entityTypeName>
            <conditions>
                <attributeName>userTypeName</attributeName>
                <operatorId>1</operatorId>
                <value>Administrator</value>
            </conditions>
        </expressions>
        <expressions>
            <entityTypeName>u</entityTypeName>
            <conditions>
                <attributeName>username</attributeName>
                <operatorId>9</operatorId>
                <value>.d</value>
            </conditions>
        </expressions>
        <expressions>
            <entityTypeName>u</entityTypeName>
            <conditions>
                <attributeName>username</attributeName>
                <operatorId>8</operatorId>
                <value>load.user</value>
            </conditions>
        </expressions>
    </ruleSet>
</groupHaul>
